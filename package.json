  },
  "engines": {
    "node": "5.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rileyjshaw/twitter-math"
  },
  "keywords": [
    "twitter",
    "math",
    "facts",
    "quiz"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13"
  }
