{
  "name": "twitter-math",
  "version": "1.0.0",
  "description": "A math fact spewing twitter bot",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon ./src/index.js --exec babel-node",
    "build": "babel ./src -d ./build",
    "serve": "npm run build && node ./build/index.js"
  },
  "dependencies": {
    "babel": "^6.3.26",
    "kefir": "^3.2.0",
    "redis": "^2.4.2",
